{
  "name": "ddkits-react-form",
  "version": "0.1.15",
  "author": "Sam Ayoub <sam@ddkits.com>",
  "license" : "MIT",
  "description": "DDKits React Form, powered by www.reallexi.com, is a custom fully component to build a dynamic fully functional form, with required, pagination and fields validation, all in one. Easy use by JSON or an object to pass. free of bugs.",
  "keywords": [
    "react",
    "components",
    "ui",
    "form",
    "custom",
    "dynamic",
    "pages",
    "submission",
    "free"
  ],
  "private": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddkits/ddkits-react-form.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "buildp": "rm -rf dist && NODE_ENV=production babel src/lib --out-dir dist --copy-files"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "*",
    "@babel/core": "*",
    "@babel/polyfill": "*",
    "@babel/preset-env": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "react-scripts": "*",
    "web-vitals": "*"
  },
  "peerDependencies": {
    "core-js": "*",
    "react": "*",
    "react-dom": "*"
  },
  "dependencies": {
    "react-spinners": "^0.13.7"
  }
}
